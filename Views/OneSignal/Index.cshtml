@model IEnumerable<AG_OneSignalAPI.Models.OneSignalDetails>

@{

    ViewBag.Title = "One Signal Apps Detail";

}

<style>
    th {
        text-align:center;
    }
</style>


<h2 style="text-align:center;font-weight:bolder">@ViewBag.Title</h2>

<p style="text-align:right;font-size:medium">
    @Html.ActionLink("Create New App", "CreateApp")
</p>
<table class="table table-bordered table-responsive table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.chrome_web_origin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.created_at)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.updated_at)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.basic_auth_key)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.chrome_web_origin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.created_at)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.updated_at)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.basic_auth_key)
            </td>
            <td>

                @Html.ActionLink("Details", "Details", new { id = item.id })
               

            </td>
        </tr>
    }

</table>
